
@article{douven_what_2020,
	title = {What are natural concepts? {A} design perspective},
	volume = {35},
	issn = {0268-1064, 1468-0017},
	shorttitle = {What are natural concepts?},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/mila.12240},
	doi = {10.1111/mila.12240},
	abstract = {Abstract
            
              Conceptual spaces have become an increasingly popular modeling tool in cognitive psychology. The core idea of the conceptual spaces approach is that concepts can be represented as regions in similarity spaces. While it is generally acknowledged that not every region in such a space represents a
              natural
              concept, it is still an open question what distinguishes those regions that represent natural concepts from those that do not. The central claim of this paper is that natural concepts are represented by the cells of an optimally designed similarity space.},
	language = {en},
