@book{kistler_esprit_2016,
	address = {Paris},
	edition = {1re édition},
	series = {Philosophie},
	title = {L'esprit matériel: réduction et émergence},
	isbn = {978-2-916120-51-5},
	shorttitle = {L'esprit matériel},
	abstract = {"Nous sommes des êtres à la fois spirituels et corporels. Mon poids est un attribut corporel, tandis que ma capacité d'imaginer un paysage marin relève d'un attribut mental. Or, une fois admise cette dualité des attributs, comment concevoir l'interaction entre le corps et l'esprit ? Face aux doctrines dualistes qui échouent à répondre à cette question, Max Kistler défend ici une variante du matérialisme réductionniste qui fait droit à la notion d'émergence : corps et esprit se logent au sein d'une hiérarchie de niveaux de réalité. Les personnes - exclusivement composées, comme tout autre objet matériel, d'atomes - ont des propriétés émergentes que ne possède aucune de leurs composantes. L'auteur développe ainsi une nouvelle conception de l'émergence qui la rend compatible avec la réduction. Une personne peut penser tandis qu'aucune partie de son corps ne le peut : en ce sens, la capacité de penser émerge au niveau de la personne. Mais il est en principe possible d'expliquer cette capacité en termes des parties physiologiques de la personne. Une telle intégration dans la conception scientifique du monde ne prive notre esprit ni de réalité ni d'efficacité : lorsque je raconte ma rêverie à un ami, ma rêverie et le désir de la partager figurent parmi les causes de la vibration de mes cordes vocales..."--Page 4 of cover},
	publisher = {Ithaque},
	author = {Kistler, Max},
	year = {2016},
	note = {OCLC: ocn966904050},
	keywords = {Emergence (Philosophy), Materialism, Mind and body, Reductionism},
	annote = {L'unité de la science et la réduction inter-théorique -- Unification déductive et ontologique -- Le modèle déductif-nomologique de la réduction -- Le modèle de la réduction par analogie -- La réduction de la thermodynamique à la mécanique statistique -- Le modèle synthétique de la réduction -- La réduction de phénomènes cognitifs par la neurophysiologie : élimination ou co-évolution ? -- Peut-on construire des explications réductrices a priori? -- La réduction a priori dans le cadre de la sémantique bidimensionnelle -- Deux concepts de réduction et de réalisation : micro-macro et rôle-occupant -- Multiréalisabilité -- Les capacités cognitives comme propriétés dispositionnelles macroscopiques -- Arguments généraux contre l'efficacité des dispositions -- Propriétés dispositionnelles et propriétés théoriques -- Le trilemme épiphénoménaliste des dispositions macroscopiques -- L'exemple de la représentation des couleurs -- Les propriétés dispositionnelles à manifestations multiples -- Les propriétés émergentes -- Conditions minimales ("émergence faible") -- Broad et la conception épistémique de l'émergence -- L'Émergence "forte", ou l'impossibilité de déduire (expliquer, prédire) -- L'émergence comme non-agrégativité -- L'Émergence en termes d'interactions non linéaires, et la distinction de Mill entre "effets homopathiques" et "hétéropathiques" -- Différence qualitative et quantitative -- Les limites de l'explication des propriétés émergentes -- Éviter le panpsychisme -- Réponse à une version de l'argument de Kripke contre la théorie de l'identité -- Émergence, réduction et survenance -- L'efficacité causale des propriétés de haut niveau -- Causalité, responsabilité causale et explication causale -- Causalité mentale et causalité "descendante" (downward causation) -- Macrocausalité en l'absence de mécanisme microscopique sous-jacent -- L'argument de Kim contre la causalité mentale : préliminaires -- Première partie de l'argument de Kim contre la causalité mentale : pas de causalité entre propriétés mentales sans causalité descendante -- Seconde partie de l'argument de Kim contre la causalité mentale : il n'y a pas de causalité descendante -- Propriétés mentales ou propriétés physiques conçues avec des concepts mentaux ?},
	annote = {Revision of the author's thesis (Habilitation)--Paris 1, 2006},
}

