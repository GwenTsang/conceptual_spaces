@phdthesis{kistler_esprit_2023,
	address = {Paris},
	title = {L'esprit matériel: réduction et émergence},
	shorttitle = {L'esprit matériel},
	abstract = {Nous sommes des êtres à la fois spirituels et corporels. Mon poids est un attribut corporel, tandis que ma capacité d'imaginer un paysage marin relève d'un attribut mental. Or, une fois admise cette dualité des attributs, comment concevoir l'interaction entre le corps et l'esprit ? Face aux doctrines dualistes qui échouent à répondre à cette question, Max Kistler défend une variante du matérialisme réductionniste qui fait droit à la notion d'émergence : corps et esprit se logent au sein d'une hiérarchie de niveaux de réalité. Les personnes – exclusivement composées, comme tout autre objet matériel, d'atomes – ont des propriétés émergentes que ne possède aucune de leurs composantes. Max Kistler développe ainsi une nouvelle conception de l'émergence, compatible avec la réduction. Une personne peut penser tandis qu'aucune partie de son corps ne le peut : la capacité de penser émerge au niveau de la personne. Mais il est en principe possible d'expliquer cette capacité en termes des parties physiologiques de la personne. Une telle intégration dans la conception scientifique du monde ne prive notre esprit ni de réalité ni d'efficacité : lorsque je raconte ma rêverie à un ami, ma rêverie et le désir de la partager figurent bien parmi les causes de la vibration de mes cordes vocales…},
	language = {fre},
	school = {Éliott},
	author = {Kistler, Max},
	year = {2023},
	note = {Edition: Nouvelle éd.
ISBN: 9782493117281
Series: Réflexion faite},
}
